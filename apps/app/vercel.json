{
    "github": {
        "silent": true
    },
    "installCommand": "rm -rf node_modules package-lock.json && npm install",
    "crons": [
        {
            "path": "/api/auto-release-commissions",
            "schedule": "0 */6 * * *"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        }
    ]
}